        <strong><%= TblQualityIssue.human_attribute_name :Issuedby %>  </strong> :&nbsp;  <%= @tbl_quality_issue.Issuedby %> &nbsp;&nbsp;&nbsp;
        <br />
        <strong><%= TblQualityIssue.human_attribute_name :Reason %>  </strong> :&nbsp;  <%= @tbl_quality_issue.Reason %> &nbsp;&nbsp;&nbsp;
        <strong><%= TblQualityIssue.human_attribute_name :ReasonNote %>  </strong> :&nbsp;  <%= @tbl_quality_issue.ReasonNote %> &nbsp;&nbsp;&nbsp;
        <br />
        <strong><%= TblQualityIssue.human_attribute_name :id %>  </strong> :&nbsp;  <%= @tbl_quality_issue.id %> &nbsp;&nbsp;&nbsp;

